{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": ["lua-types/jit"],
    "strict": true
  },
  "tstl": {
    "luaTarget": "JIT",
    "luaLibImport":"inline"
  }
}